  },
  "homepage": "https://github.com/ddneat/tropic#readme",
  "keywords": [
    "tdd",
    "unit-testing",
    "test-runner",
    "assert",
    "async",
    "promise",
    "babel",
    "mocha",
    "jest",
    "ava",
    "tape",
    "tap",
    "qunit",
    "selenium",
    "jasmine",
    "chai",
    "minimal"
  ],
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@babel/preset-env": "^7.8.7",
